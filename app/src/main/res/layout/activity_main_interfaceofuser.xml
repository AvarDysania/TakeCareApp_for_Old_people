<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainInterfaceofuser">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="135dp"
        android:layout_height="39dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="52dp"
        android:fontFamily="sans-serif-black"
        android:text="My Profile"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="104dp"
        android:layout_height="42dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="9dp"
        android:fontFamily="@font/blinker_semibold"
        android:text=" username:"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="147dp"
        android:layout_height="34dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="9dp"
        android:fontFamily="sans-serif-black"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="46dp"
        android:layout_height="47dp"
        android:layout_marginStart="136dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/blinker_semibold"
        android:text="age:"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="140dp"
        android:layout_height="30dp"
        android:layout_marginStart="7dp"
        android:layout_marginTop="10dp"
        android:fontFamily="sans-serif-black"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="105dp"
        android:layout_height="91dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="43dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/old" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="215dp"
        android:layout_height="38dp"
        android:layout_marginStart="114dp"
        android:layout_marginTop="31dp"
        android:fontFamily="@font/alata"
        android:text="Welcome to Safe Home!"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="154dp"
        android:layout_height="174dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:onClick="healthCheck"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
        app:srcCompat="@drawable/healthcheck" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="185dp"
        android:layout_height="164dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="19dp"
        android:onClick="medication"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
        app:srcCompat="@drawable/medication" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="107dp"
        android:layout_height="31dp"
        android:layout_marginStart="42dp"
        android:layout_marginBottom="248dp"
        android:fontFamily="@font/fredoka_one"
        android:text="Health Check"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="107dp"
        android:layout_height="31dp"
        android:layout_marginEnd="59dp"
        android:layout_marginBottom="247dp"
        android:fontFamily="@font/fredoka_one"
        android:text="Medication"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>