<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/doct"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".PatientHelp">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="81dp"
        android:layout_height="82dp"
        android:layout_marginStart="117dp"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/alata"
        android:text="Elderly "
        android:textAlignment="center"
        android:textColor="#151212"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="80dp"
        android:layout_height="68dp"
        android:layout_marginTop="24dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/alata"
        android:text="Care"
        android:textAlignment="center"
        android:textColor="#171C17"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="71dp"
        android:layout_height="54dp"
        android:layout_marginStart="39dp"
        android:layout_marginTop="24dp"
        android:foregroundTint="#0B370D"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_call" />

    <com.airbnb.lottie.LottieAnimationView
        android:layout_width="372dp"
        android:layout_height="533dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_speed="@integer/material_motion_duration_short_1"
        app:lottie_rawRes="@raw/doctor" />

</androidx.constraintlayout.widget.ConstraintLayout>